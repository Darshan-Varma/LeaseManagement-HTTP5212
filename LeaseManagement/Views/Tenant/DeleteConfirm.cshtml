@model LeaseManagement.Models.TenantDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Tenant?</h2>
<a href="/Tenant/List">Cancel</a>
<h3>Name: @Model.TenantName </h3>
<form action="/Tenant/Delete/@Model.TenantId" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>